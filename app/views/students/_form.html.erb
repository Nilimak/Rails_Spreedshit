<% if @students.present? %>
<%students=[]%>
<% @students.each do |s|%>
<% students << s %>
<%end%>
<%end%>
<%= form_for(@student) do |f| %>
  
<%= link_to 'Excel Report',excel_report_students_path(format:'xls',:s_name=>students),class: 'btn btn-success'%>
<%= link_to 'PDF',{:action=>'pdf_report',format:'pdf'},:target => 'blank', method: :post,class: "btn btn-success" %>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
